<!-- Basic Card Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Editando Membro</h6>
    </div>
    <div class="card-body">
        <%= form_with(model: [:admins_backoffice, @churchmember]) do |form| %>
            <div class="form-group">
            <%= form.label :name %>
            <%= form.text_field :name ,class:"form-control form-control-user", placeholder:"Nome" %>
            </div>

            <div class="form-group">
            <%= form.label :email %>
            <%= form.text_field :email ,class:"form-control form-control-user", placeholder:"exemplo@exemplo.com" %>
            </div>

            <div class="form-group">
            <%= form.label :cellphone %>
            <%= form.text_field :cellphone ,class:"form-control form-control-user", placeholder:"67 99999-9999" %>
            </div>

            <div class="form-group">
            <%= form.label :birthdate %>
            <%= form.date_field :birthdate ,class:"form-control form-control-user"%>
            </div>

            <div class="form-group">
            <%= form.label :membershipdate %>
            <%= form.date_field :membershipdate ,class:"form-control form-control-user" %>
            </div>

            <div class="form-group">
            <%= form.label :street %>
            <%= form.text_field :street ,class:"form-control form-control-user", placeholder:"Rua: Afonso Pena, 1200" %>
            </div>

            <div class="form-group">
            <%= form.label :district %>
            <%= form.text_field :district ,class:"form-control form-control-user", placeholder:"Centro" %>
            </div>

            <button type="submit" class="btn btn-primary btn-user btn-block">Salvar</button>
        <% end %>
    </div>
</div>